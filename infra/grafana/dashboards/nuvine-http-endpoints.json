{
  "id": null,
  "uid": "nuvine-http-endpoints",
  "title": "Nuvine HTTP Endpoints",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "application",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 1,
        "query": "label_values(http_server_requests_seconds_count, application)",
        "current": {
          "text": "All",
          "value": ".*",
          "selected": true
        },
        "includeAll": true,
        "multi": true,
        "label": "application",
        "hide": 0,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "type": "bargauge",
      "title": "RPS by URI (last 1m)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{application=~\"$application\"}[1m])) by (uri)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "p95 latency by URI [ms]",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "1000 * histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{application=~\"$application\"}[5m])) by (le, uri))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Error rate by URI (5xx / all)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 10
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "showHeader": true
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\",application=~\"$application\"}[5m])) by (uri) / sum(rate(http_server_requests_seconds_count{application=~\"$application\"}[5m])) by (uri)",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ]
    }
  ]
}